' TestConnList.vbs
On Error Resume Next
WScript.Echo "Script host: " & WScript.FullName

Dim connList
Set connList = CreateObject("PCOMM.autECLConnList")
If Err.Number <> 0 Then
  WScript.Echo "CreateObject failed: " & Err.Number & " - " & Err.Description
  WScript.Quit 1
End If

WScript.Echo "autECLConnList created OK."
WScript.Echo "Count = " & connList.Count

Dim i
For i = 1 To connList.Count
    WScript.Echo "Connection(" & i & ") = " & connList(i).Name
Next
